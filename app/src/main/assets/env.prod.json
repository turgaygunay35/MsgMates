{
  "baseUrl": "https://api.msgmates.com/",
  "wsUrl": "wss://api.msgmates.com/ws",
  "auth": {
    "requestCode": "auth/request-code",
    "verifyCode": "auth/verify-code",
    "loginPassword": "v1/auth/login/password",
    "refresh": "v1/auth/refresh",
    "revoke": "v1/auth/revoke",
    "registerDevice": "v1/device/register"
  },
  "timeouts": {
    "connect": 30000,
    "read": 30000,
    "write": 30000
  },
  "tls": {
    "certificatePins": [
      "sha256/AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA="
    ]
  },
  "wsCfg": {
    "pingInterval": 30000,
    "reconnectDelay": 5000,
    "maxReconnectAttempts": 5
  }
}









